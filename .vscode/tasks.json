{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "go",
			"label": "go: build selkieman-compose",
			"command": "build",
			"args": [
				"./..."
			],
			"problemMatcher": [
				"$go"
			],
			"group": "build"
		},
		{
			"type": "shell",
			"label": "go: run selkieman-compose",
			"command": "${workspaceFolder}/selkieman-compose",
			"args": [],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none",
			"dependsOn": [
				"go: build selkieman-compose"
			]
		},
		{
			"type": "shell",
			"label": "python3: test",
			"command": "python3 ${workspaceFolder}/test.py",
			"args": [],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test"
		},
		{
			"type": "shell",
			"label": "podman-compose version",
			"command": "podman-compose version",
			"group": "none",
			"problemMatcher": []
		}
	]
}